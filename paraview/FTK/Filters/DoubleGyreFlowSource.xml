<ServerManagerConfiguration>
  <ProxyGroup name="sources">
    <SourceProxy name="DoubleGyreFlowSource" class="vtkDoubleGyreFlowSource" label="DoubleGyreFlowSource">
      <Documentation
        short_help="synthetic 2D ABC flow data for FTK"
        long_help="synthetic 2D ABC flow data for FTK">
      </Documentation>

      <IntVectorProperty 
        name="Width"
        command="setW"
        number_of_elements="1"
        default_values="64">
      </IntVectorProperty>
      
      <IntVectorProperty 
        name="Height"
        command="setH"
        number_of_elements="1"
        default_values="32">
      </IntVectorProperty>
      
      <DoubleVectorProperty
        name="Time"
        command="setTime"
        number_of_elements="1"
        default_values="0.1">
      </DoubleVectorProperty>
      
      <DoubleVectorProperty
        name="A"
        command="setA"
        number_of_elements="1"
        default_values="0.1">
      </DoubleVectorProperty>
      
      <DoubleVectorProperty
        name="Omega"
        command="setOmega"
        number_of_elements="1"
        default_values="6.28318530718">
      </DoubleVectorProperty>
      
      <DoubleVectorProperty
        name="Epsilon"
        command="setEpsilon"
        number_of_elements="1"
        default_values="0.25">
      </DoubleVectorProperty>

      <Hints>
        <ShowInMenu category="FTK"/>
      </Hints>
    </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>
