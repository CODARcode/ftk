<ServerManagerConfiguration>
  <ProxyGroup name="sources">
    <SourceProxy name="DoubleGyre2DSource" class="vtkDoubleGyre2DSource" label="DoubleGyre2DSource">
      <Documentation
        short_help="synthetic 2D double gyre flow data for FTK"
        long_help="synthetic 2D double gyre data for FTK">
      </Documentation>

      <IntVectorProperty 
        name="Width"
        command="setW"
        number_of_elements="1"
        default_values="64">
      </IntVectorProperty>
      
      <IntVectorProperty 
        name="Height"
        command="setH"
        number_of_elements="1"
        default_values="32">
      </IntVectorProperty>
      
      <IntVectorProperty 
        name="Timesteps"
        command="setT"
        number_of_elements="1"
        default_values="100">
      </IntVectorProperty>
      
      <DoubleVectorProperty
        name="StartTime"
        command="setStartTime"
        number_of_elements="1"
        default_values="0.0">
      </DoubleVectorProperty>
      
      <DoubleVectorProperty
        name="TimeScale"
        command="setTimeScale"
        number_of_elements="1"
        default_values="0.1">
      </DoubleVectorProperty>
      
      <DoubleVectorProperty
        name="A"
        command="setA"
        number_of_elements="1"
        default_values="0.1">
      </DoubleVectorProperty>
      
      <DoubleVectorProperty
        name="Omega"
        command="setOmega"
        number_of_elements="1"
        default_values="6.28318530718">
      </DoubleVectorProperty>
      
      <DoubleVectorProperty
        name="Epsilon"
        command="setEpsilon"
        number_of_elements="1"
        default_values="0.25">
      </DoubleVectorProperty>

			<DoubleVectorProperty
			  information_only="1"
			  name="TimestepValues"
        repeatable="1">
        <TimeStepsInformationHelper/>
			  <Documentation>
			    Available timestep values.
			  </Documentation>
			</DoubleVectorProperty>

      <Hints>
        <ShowInMenu category="FTK"/>
      </Hints>
    </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>
